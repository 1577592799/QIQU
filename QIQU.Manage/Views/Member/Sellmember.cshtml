@using UMould.TLM.Manage.Model

<div class="row">
    <div class="col-lg-12">
        <div class="row">
            <div class="col-lg-12">
                <ol class="breadcrumb">
                    <li><a href="#">首页</a></li>
                    <li><a href="/Member/Datainformation/" data-ajax-update="#main_content" data-ajax-mode="replace" data-ajax="true">数据信息列表</a></li>
                    <li class="active"><span>数据信息查看</span></li>
                </ol>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="main-box clearfix">
                    <header class="main-box-header clearfix">
                        <h2 class="pull-left">数据信息</h2>
                    </header>
                    <div style="text-align: left;margin:10px;" class="panel">
                        <!--用户信息-->
                        <div class="panel-heading">
                            <b>数据信息</b>
                        </div>
                        <table class="table">
                            <tbody>
                                @if (ViewBag.models == null)
                                {
                                    <tr style="text-align:center;">
                                        <th>
                                            <font color="red">没有找到数据信息</font>
                                        </th>
                                        <td></td>
                                        <td></td>
                                    </tr>
                                }
                                else
                                {
                                    foreach (var m in ViewBag.models)
                                    {

                                        <tr>
                                            <th class="center" style="width:200px;">序号</th>
                                            <td style="width:500px;" class="w-p50">
                                                <label>@m.Id</label>
                                            </td>
                                            <td></td>
                                        </tr>
                                        <tr>
                                            <th class="center">域名</th>
                                            <td>
                                                @m.Domain
                                            </td>
                                            <td></td>
                                        </tr>
                                        <tr>
                                            <th class="center">数据库名称</th>
                                            <td>
                                                @m.DBName
                                            </td>
                                            <td></td>
                                        </tr>
                                        <tr>
                                            <th class="center">数据源</th>
                                            <td>
                                                @m.DBSource
                                        </td>
                                        <td></td>
                                    </tr>
                                    }
                                }
                            </tbody>
                        </table>
                        <p>
                            <a class="btn btn-default" style="width:100px;margin-left: 10px;margin-right:10px;" href="/Member/Datainformation/" data-ajax-update="#main_content" data-ajax-mode="replace" data-ajax="true">返 回</a>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
